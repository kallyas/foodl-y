<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="findRestaurantsVM" type="com.foodly.app.modules.findrestaurants.data.viewmodel.FindRestaurantsVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackrectangle"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_gravity="start" android:layout_height="0dp"
            android:layout_weight="1" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frame49FindRestaur"
                android:layout_height="match_parent" android:layout_width="match_parent">
                <FrameLayout android:id="@+id/frameStackmap"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_768pxv"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent" style="@style/groupStylebluegray_100">
                    <ImageView android:id="@+id/imageMap"
                        android:layout_gravity="start"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_map}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_map"/>
                    <LinearLayout android:id="@+id/linearColumnpin"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <androidx.appcompat.widget.Toolbar
                            android:gravity="start"
                            android:id="@+id/toolbarToolbar"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" app:contentInsetStart="0dp">
                            <FrameLayout android:id="@+id/frameStackpin"
                                android:layout_height="@dimen/_34pxv" android:layout_width="match_parent">
                                <ImageButton
                                    android:background="@drawable/rectangle_bg_orange_200_border_white_a700_radius_8"
                                    android:id="@+id/btnPin"
                                    android:layout_gravity="bottom|start"
                                    android:layout_height="@dimen/_16pxh"
                                    android:layout_marginBottom="@dimen/_8pxv"
                                    android:layout_marginEnd="@dimen/_144pxh"
                                    android:layout_marginStart="@dimen/_144pxh"
                                    android:layout_marginTop="@dimen/_10pxv"
                                    android:layout_width="@dimen/_16pxh"
                                    android:paddingHorizontal="@dimen/_3pxh"
                                    android:paddingVertical="@dimen/_3pxv"
                                    android:scaleType="fitCenter"
                                    android:src="@drawable/img_pin" tools:ignore="ContentDescription"/>
                                <LinearLayout
                                    android:id="@+id/linearTopbar"
                                    android:layout_gravity="start"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent" android:orientation="horizontal">
                                    <ImageButton
                                    android:background="@drawable/rectangle_bg_white_a700_radius_17"
                                    android:id="@+id/btnArrowleft"
                                    android:layout_height="@dimen/_34pxh"
                                    android:layout_width="@dimen/_34pxh"
                                    android:paddingHorizontal="@dimen/_10pxh"
                                    android:paddingVertical="@dimen/_10pxv"
                                    android:scaleType="fitCenter"
                                    android:src="@drawable/img_arrowleft" tools:ignore="ContentDescription"/>
                                    <ImageButton
                                    android:background="@drawable/rectangle_bg_white_a700_radius_17"
                                    android:id="@+id/btnSearch"
                                    android:layout_height="@dimen/_34pxh"
                                    android:layout_marginStart="@dimen/_267pxh"
                                    android:layout_width="@dimen/_34pxh"
                                    android:paddingHorizontal="@dimen/_9pxh"
                                    android:paddingVertical="@dimen/_9pxv"
                                    android:scaleType="fitCenter"
                                    android:src="@drawable/img_search" tools:ignore="ContentDescription"/>
                                </LinearLayout>
                            </FrameLayout>
                        </androidx.appcompat.widget.Toolbar>
                        <LinearLayout android:gravity="end"
                            android:id="@+id/linearRowpincopytwo"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_20pxh"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageButton
                                android:background="@drawable/rectangle_bg_deep_purple_a100_border_white_a700_radius_8"
                                android:id="@+id/btnPincopyTwo"
                                android:layout_height="@dimen/_16pxh"
                                android:layout_marginTop="@dimen/_226pxv"
                                android:layout_width="@dimen/_16pxh"
                                android:paddingHorizontal="@dimen/_3pxh"
                                android:paddingVertical="@dimen/_3pxv"
                                android:scaleType="fitCenter"
                                android:src="@drawable/img_pin" tools:ignore="ContentDescription"/>
                            <LinearLayout android:id="@+id/linearRowway"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_14pxv"
                                android:layout_marginStart="@dimen/_78pxh"
                                android:layout_weight="0.91"
                                android:layout_width="0dp" android:orientation="horizontal">
                                <ImageView android:id="@+id/imageWay"
                                    android:layout_height="@dimen/_228pxh"
                                    android:layout_width="@dimen/_78pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_way"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_way"/>
                                <ImageButton
                                    android:background="@drawable/rectangle_bg_indigo_a100_border_white_a700_radius_8"
                                    android:id="@+id/btnPincopy"
                                    android:layout_height="@dimen/_16pxh"
                                    android:layout_marginBottom="@dimen/_73pxv"
                                    android:layout_marginStart="@dimen/_5pxh"
                                    android:layout_marginTop="@dimen/_139pxv"
                                    android:layout_width="@dimen/_16pxh"
                                    android:paddingHorizontal="@dimen/_3pxh"
                                    android:paddingVertical="@dimen/_3pxv"
                                    android:scaleType="fitCenter"
                                    android:src="@drawable/img_pin" tools:ignore="ContentDescription"/>
                                <ImageButton
                                    android:background="@drawable/rectangle_bg_deep_orange_a100_border_white_a700_radius_8"
                                    android:id="@+id/btnPincopyThree"
                                    android:layout_height="@dimen/_16pxh"
                                    android:layout_marginBottom="@dimen/_39pxv"
                                    android:layout_marginStart="@dimen/_44pxh"
                                    android:layout_marginTop="@dimen/_173pxv"
                                    android:layout_width="@dimen/_16pxh"
                                    android:paddingHorizontal="@dimen/_3pxh"
                                    android:paddingVertical="@dimen/_3pxv"
                                    android:scaleType="fitCenter"
                                    android:src="@drawable/img_pin" tools:ignore="ContentDescription"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
                <LinearLayout android:id="@+id/linearTopRestaurants"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                    <View
                        android:background="@drawable/rectangle_bg_gray_200_radius_2"
                        android:id="@+id/viewRectangle"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_4pxv"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_12pxv" android:layout_width="@dimen/_34pxh"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTopPickRestau"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_27pxv"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{findRestaurantsVM.findRestaurantsModel.txtTopPickRestau}"
                        style="@style/txtSFProDisplaysemibold24" tools:text="@string/msg_top_pick_restau"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearFoodtype"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_17pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtCTASecondery"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.32"
                            android:layout_width="0dp"
                            android:letterSpacing="0.05"
                            android:paddingBottom="@dimen/_12pxv"
                            android:paddingEnd="@dimen/_21pxh"
                            android:paddingStart="@dimen/_20pxh"
                            android:paddingTop="@dimen/_12pxv"
                            android:singleLine="true"
                            android:text="@{findRestaurantsVM.findRestaurantsModel.txtCTASecondery}"
                            android:textAllCaps="true"
                            style="@style/txtSolidRounded" tools:text="@string/lbl_burgers"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtCTASeconderyOne"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_weight="0.31"
                            android:layout_width="0dp"
                            android:letterSpacing="0.05"
                            android:paddingBottom="@dimen/_12pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_21pxh"
                            android:paddingTop="@dimen/_12pxv"
                            android:singleLine="true"
                            android:text="@{findRestaurantsVM.findRestaurantsModel.txtCTASeconderyOne}"
                            android:textAllCaps="true"
                            style="@style/txtSolidRounded_1" tools:text="@string/lbl_brunch"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtCTASeconderyC"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_weight="0.37"
                            android:layout_width="0dp"
                            android:letterSpacing="0.05"
                            android:paddingBottom="@dimen/_12pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_22pxh"
                            android:paddingTop="@dimen/_12pxv"
                            android:singleLine="true"
                            android:text="@{findRestaurantsVM.findRestaurantsModel.txtCTASeconderyC}"
                            android:textAllCaps="true"
                            style="@style/txtSolidRounded_1" tools:text="@string/lbl_breakfast"/>
                    </LinearLayout>
                    <View android:background="@color/gray_700_19"
                        android:id="@+id/lineDivider"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_24pxv" android:layout_width="@dimen/_335pxh"/>
                    <androidx.recyclerview.widget.RecyclerView
                        android:gravity="center_horizontal"
                        android:id="@+id/recyclerGridbg"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_18pxv"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="match_parent"
                        android:nestedScrollingEnabled="false"
                        app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
                        app:spanCount="2" itemSpace="@{@dimen/_15pxh}"
                        tools:itemCount="6" tools:listitem="@layout/row_gridbg"/>
                </LinearLayout>
            </FrameLayout>
        </androidx.core.widget.NestedScrollView>
    </FrameLayout>
</layout>
