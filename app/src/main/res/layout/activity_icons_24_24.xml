<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="icons2424VM" type="com.foodly.app.modules.icons2424.data.viewmodel.Icons2424VM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearIcons2424"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <TextView android:ellipsize="end" android:id="@+id/txtLanguage"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_60pxh"
            android:layout_marginStart="@dimen/_60pxh"
            android:layout_marginTop="@dimen/_54pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{icons2424VM.icons2424Model.txtLanguage}"
            style="@style/txtSFProDisplaymedium34_1" tools:text="@string/lbl_icons_24_24"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnarrowleft"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_60pxh"
            android:layout_marginStart="@dimen/_60pxh"
            android:layout_marginTop="@dimen/_39pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_3pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_8pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <ImageView android:id="@+id/imageLock"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_53pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_lock"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                <ImageView android:id="@+id/imageLockOne"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_50pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_15pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_lock_15x15"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_lock_15x15"/>
                <ImageView android:id="@+id/imageMobile"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_mobile"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_mobile"/>
                <ImageView android:id="@+id/imageNotification"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_48pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_notification"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_notification"/>
                <ImageView android:id="@+id/imageFacebook"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_52pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_7pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_facebook_14x7"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_facebook_14x7"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowvolume"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_44pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVolume"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_volume_20x20"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_volume_20x20"/>
                <ImageView android:id="@+id/imageClock"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_46pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_clock_1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_clock_1"/>
                <ImageView android:id="@+id/imageStar"
                    android:layout_height="@dimen/_13pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_star_16x16"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_star_16x16"/>
                <ImageView android:id="@+id/imageSend"
                    android:layout_height="@dimen/_19pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_send_25x25"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_send_25x25"/>
                <ImageView android:id="@+id/imageEye"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_46pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_12pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_eye"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_eye"/>
                <ImageView android:id="@+id/imageSearch"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_50pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_search"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowcamera"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_46pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <View android:background="@color/black_935"
                    android:id="@+id/viewCamera"
                    android:layout_height="@dimen/_13pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_19pxh"/>
                <View
                    android:background="@drawable/rectangle_bg_black_935"
                    android:id="@+id/viewRectangle185"
                    android:layout_height="@dimen/_2pxv"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginStart="@dimen/_47pxh"
                    android:layout_marginTop="@dimen/_7pxv" android:layout_width="@dimen/_14pxh"/>
                <ImageView android:id="@+id/imagePlus"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_plus"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_plus"/>
                <ImageView android:id="@+id/imageArrowright"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_52pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_8pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_black_935"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_black_935"/>
                <ImageView android:id="@+id/imageReply"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_52pxh"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_reply_16x16"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_reply_16x16"/>
                <ImageView android:id="@+id/imageMobileOne"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_mobile_17x16"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_mobile_17x16"/>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerListfolder"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_47pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_46pxh}" tools:itemCount="2" tools:listitem="@layout/row_listfolder"/>
            <ImageView android:id="@+id/imageHome"
                android:layout_height="@dimen/_14pxh"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_48pxv"
                android:layout_width="@dimen/_15pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_home_17x18"
                tools:ignore="ContentDescription" tools:src="@drawable/img_home_17x18"/>
        </LinearLayout>
    </LinearLayout>
</layout>
