<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="shareLinkPopupVM" type="com.foodly.app.modules.sharelinkpopup.data.viewmodel.ShareLinkPopupVM"/>
    </data>
    <FrameLayout android:id="@+id/frame46SharelinkP"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearMain"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                app:contentInsetStart="0dp" style="@style/groupStylewhite_A700">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_128pxh"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_8pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtRefertoFriend"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_93pxh"
                        android:layout_weight="0.94"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{shareLinkPopupVM.shareLinkPopupModel.txtRefertoFriend}"
                        style="@style/txtSFProTextsemibold16" tools:text="@string/msg_refer_to_friend"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <ImageView android:id="@+id/imageIcon"
                android:layout_gravity="start"
                android:layout_height="@dimen/_146pxh"
                android:layout_marginEnd="@dimen/_74pxh"
                android:layout_marginStart="@dimen/_74pxh"
                android:layout_marginTop="@dimen/_37pxv"
                android:layout_width="@dimen/_135pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_icon"
                tools:ignore="ContentDescription" tools:src="@drawable/img_icon"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtPrice"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_28pxv"
                android:layout_width="match_parent"
                android:letterSpacing="0.01"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{shareLinkPopupVM.shareLinkPopupModel.txtPrice}"
                style="@style/txtSFProDisplaysemibold24" tools:text="@string/msg_refer_a_friend"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtPriceOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:text="@{shareLinkPopupVM.shareLinkPopupModel.txtPriceOne}"
                style="@style/txtSFProTextregular16" tools:text="@string/msg_get_10_in_cred"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearSharelink"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_36pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_1">
                <ImageView android:id="@+id/imageReply"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_reply"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_reply"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtWeburl"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_41pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_weight="0.91"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{shareLinkPopupVM.shareLinkPopupModel.txtWeburl}"
                    style="@style/txtSFProTextregular16_4" tools:text="@string/msg_https_ui8_net"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearCTAPrimary"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_203pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegreen_1">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtEmail"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_marginEnd="@dimen/_144pxh"
                    android:layout_marginStart="@dimen/_144pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.04"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{shareLinkPopupVM.shareLinkPopupModel.txtEmail}"
                    android:textAllCaps="true"
                    style="@style/txtSFProTextbold14" tools:text="@string/lbl_email"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearScancard"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_1">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtScanCard"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_137pxh"
                    android:layout_marginStart="@dimen/_138pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.06"
                    android:singleLine="true"
                    android:text="@{shareLinkPopupVM.shareLinkPopupModel.txtScanCard}"
                    android:textAllCaps="true"
                    style="@style/txtSFProDisplaysemibold14" tools:text="@string/lbl_others"/>
            </LinearLayout>
        </LinearLayout>
        <ImageView android:id="@+id/imageOverride"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_override}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_override"/>
    </FrameLayout>
</layout>
