<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="locationsVM" type="com.foodly.app.modules.locations.data.viewmodel.LocationsVM"/>
    </data>
    <LinearLayout android:id="@+id/linear43Locations"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearBarsTopbars1"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="@dimen/_8pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPaymentMethods"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginEnd="@dimen/_120pxh"
                    android:layout_marginStart="@dimen/_84pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_weight="0.94"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{locationsVM.locationsModel.txtPaymentMethods}"
                    style="@style/txtSFProTextsemibold16" tools:text="@string/lbl_payment_methods"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <EditText
            android:background="@drawable/rectangle_bg_gray_50_border_bluegray_400_1a_radius_8"
            android:drawablePadding="@dimen/_17pxh"
            android:drawableStart="@drawable/img_location"
            android:ellipsize="end"
            android:hint="@string/msg_search_new_addr"
            android:id="@+id/etSearchform" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:paddingBottom="@dimen/_15pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_22pxh"
            android:paddingTop="@dimen/_16pxv" android:singleLine="true"
            android:text="@={locationsVM.locationsModel.etSearchformValue}"
            android:textColorHint="@color/bluegray_400"
            style="@style/etSolidRoundedOutline_2" tools:ignore="Autofill"/>
        <LinearLayout android:id="@+id/linearRowupcomingorders"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_28pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtUpcomingorders"
                android:layout_height="wrap_content"
                android:layout_weight="0.69" android:layout_width="0dp"
                android:letterSpacing="0.03"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{locationsVM.locationsModel.txtUpcomingorders}"
                android:textAllCaps="true"
                style="@style/txtSFProTextmedium16_1" tools:text="@string/lbl_recent_address"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtClearall"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginStart="@dimen/_114pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.31" android:layout_width="0dp"
                android:letterSpacing="0.02"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{locationsVM.locationsModel.txtClearall}"
                android:textAllCaps="true"
                style="@style/txtSFProTextmedium12_4" tools:text="@string/lbl_clear_all"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowlocation"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_26pxh"
            android:layout_marginStart="@dimen/_26pxh"
            android:layout_marginTop="@dimen/_28pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageLocation"
                android:layout_height="@dimen/_14pxh"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_width="@dimen/_12pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_location"
                tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSanFrancisco"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.91" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{locationsVM.locationsModel.txtSanFrancisco}"
                style="@style/txtSFProTextregular16_4" tools:text="@string/msg_san_francisco2"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowlocationOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_26pxh"
            android:layout_marginStart="@dimen/_26pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageLocationOne"
                android:layout_height="@dimen/_14pxh"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_width="@dimen/_12pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_location"
                tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSanFranciscoC"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.93" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{locationsVM.locationsModel.txtSanFranciscoC}"
                style="@style/txtSFProTextregular16_4" tools:text="@string/msg_san_francisco_c"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowlocationTwo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_26pxh"
            android:layout_marginStart="@dimen/_26pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageLocationTwo"
                android:layout_height="@dimen/_14pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_12pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_location"
                tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSanFranciscoZ"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_weight="0.91" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{locationsVM.locationsModel.txtSanFranciscoZ}"
                style="@style/txtSFProTextregular16_4" tools:text="@string/msg_san_francisco_z"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowlocationThree"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_436pxv"
            android:layout_marginEnd="@dimen/_26pxh"
            android:layout_marginStart="@dimen/_26pxh"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageLocationThree"
                android:layout_height="@dimen/_14pxh"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_width="@dimen/_12pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_location"
                tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSanFranciscoOne"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.93" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{locationsVM.locationsModel.txtSanFranciscoOne}"
                style="@style/txtSFProTextregular16_4" tools:text="@string/msg_san_francisco3"/>
        </LinearLayout>
    </LinearLayout>
</layout>
