<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="icons3434VM" type="com.foodly.app.modules.icons3434.data.viewmodel.Icons3434VM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearIcons3434"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <TextView android:ellipsize="end" android:id="@+id/txtLanguage"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_60pxh"
            android:layout_marginStart="@dimen/_60pxh"
            android:layout_marginTop="@dimen/_54pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{icons3434VM.icons3434Model.txtLanguage}"
            style="@style/txtSFProDisplaymedium34_1" tools:text="@string/lbl_icons_34_34"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnlock"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_83pxv"
            android:layout_marginEnd="@dimen/_60pxh"
            android:layout_marginStart="@dimen/_60pxh"
            android:layout_marginTop="@dimen/_41pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerListlock"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_52pxh}" tools:itemCount="4" tools:listitem="@layout/row_listlock"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRoweyeOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_54pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageEyeOne"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_eye_12x18"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_eye_12x18"/>
                <ImageView android:id="@+id/imageEyeTwo"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_56pxh"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_eye_black_935"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_eye_black_935"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
