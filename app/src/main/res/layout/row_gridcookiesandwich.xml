<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="gridcookiesandwichRowModel" type="com.foodly.app.modules.searchrestaurants.data.model.GridcookiesandwichRowModel"/>
    </data>
    <FrameLayout android:id="@+id/frameFoodcardsmall"
        android:layout_height="wrap_content" android:layout_width="match_parent">
        <LinearLayout android:id="@+id/linearColumncookiesandwich"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_2">
            <TextView android:ellipsize="end"
                android:id="@+id/txtCookieSandwich"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_156pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{gridcookiesandwichRowModel.txtCookieSandwich}"
                style="@style/txtSFProTextmedium16" tools:text="@string/lbl_the_halal_guys"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearType"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFortyFour"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.24"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{gridcookiesandwichRowModel.txtFortyFour}"
                    style="@style/txtSFProTextregular14" tools:text="@string/lbl"/>
                <ImageView android:id="@+id/imageOvalCopyThree"
                    android:layout_height="@dimen/_4pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_4pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_ovalcopy3"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ovalcopy3"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_weight="0.7"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{gridcookiesandwichRowModel.txtLanguage}"
                    style="@style/txtSFProTextregular14" tools:text="@string/lbl_chinese"/>
            </LinearLayout>
        </LinearLayout>
        <ImageView android:id="@+id/imageBg"
            android:layout_gravity="top|start"
            android:layout_height="@dimen/_140pxh"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="@dimen/_160pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_8pxh}"
            imageRes="@{@drawable/img_bg_140x160}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_bg_140x160"/>
    </FrameLayout>
</layout>
