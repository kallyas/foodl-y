                    android:id="@+id/frameHeaderarea"
                    android:layout_height="@dimen/_236pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageHeaderimage"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_236pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_headerimage}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_headerimage"/>
                    <LinearLayout android:id="@+id/linearRowarrowleft"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_20pxv"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_31pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_marginBottom="@dimen/_178pxv"
                            android:layout_width="@dimen/_12pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_white_a700"/>
