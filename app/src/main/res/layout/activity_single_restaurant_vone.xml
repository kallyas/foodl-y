                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMayfieldBakery"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{singleRestaurantVoneVM.singleRestaurantVoneModel.txtMayfieldBakery}"
                        style="@style/txtSFProDisplaysemibold24_1" tools:text="@string/msg_mayfield_bakery"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearType"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTen"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.09"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{singleRestaurantVoneVM.singleRestaurantVoneModel.txtTen}"
                            style="@style/txtSFProTextregular16" tools:text="@string/lbl"/>
                        <View
                            android:background="@drawable/rectangle_bg_bluegray_400_87_radius_2"
                            android:id="@+id/viewOvalCopyThree"
                            android:layout_height="@dimen/_4pxh"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_marginTop="@dimen/_7pxv" android:layout_width="@dimen/_4pxh"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLanguage"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.25"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{singleRestaurantVoneVM.singleRestaurantVoneModel.txtLanguage}"
                            style="@style/txtSFProTextregular16" tools:text="@string/lbl_chinese"/>
                        <View
                            android:background="@drawable/rectangle_bg_bluegray_400_87_radius_2"
                            android:id="@+id/viewOvalCopyTwo"
                            android:layout_height="@dimen/_4pxh"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_marginTop="@dimen/_7pxv" android:layout_width="@dimen/_4pxh"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAmerican"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_weight="0.29"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{singleRestaurantVoneVM.singleRestaurantVoneModel.txtAmerican}"
                            style="@style/txtSFProTextregular16" tools:text="@string/lbl_american"/>
                        <View
                            android:background="@drawable/rectangle_bg_bluegray_400_87_radius_2"
                            android:id="@+id/viewOvalCopy"
                            android:layout_height="@dimen/_4pxh"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_marginTop="@dimen/_7pxv" android:layout_width="@dimen/_4pxh"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDeshifood"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_weight="0.33"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{singleRestaurantVoneVM.singleRestaurantVoneModel.txtDeshifood}"
                            style="@style/txtSFProTextregular16" tools:text="@string/lbl_deshi_food"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRating"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_23pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLanguageOne"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_weight="0.18"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{singleRestaurantVoneVM.singleRestaurantVoneModel.txtLanguageOne}"
                            style="@style/txtSFProTextmedium12" tools:text="@string/lbl_4_3"/>
                        <ImageView android:id="@+id/imageStar"
                            android:layout_height="@dimen/_11pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_9pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_11pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_star"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txt200Ratings"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.71"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{singleRestaurantVoneVM.singleRestaurantVoneModel.txt200Ratings}"
                            style="@style/txtSFProTextmedium12" tools:text="@string/lbl_200_ratings"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowfire"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginTop="@dimen/_27pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/recyclerListfire"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_weight="0.59"
                            android:layout_width="0dp"
                            android:nestedScrollingEnabled="false"
                            android:orientation="horizontal"
                            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                            itemSpace="@{@dimen/_20pxh}"
                            tools:itemCount="2" tools:listitem="@layout/row_listfire"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnTakeAway"
                            android:layout_height="@dimen/_38pxv"
                            android:layout_marginStart="@dimen/_57pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="@dimen/_113pxh"
                            android:letterSpacing="0.04"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:paddingBottom="@dimen/_13pxv"
                            android:paddingEnd="@dimen/_17pxh"
                            android:paddingStart="@dimen/_18pxh"
                            android:paddingTop="@dimen/_13pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_take_away"
                            android:textAllCaps="true"
                            style="@style/btnSolidRoundedOutline" tools:text="@string/lbl_take_away"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:gravity="start"
