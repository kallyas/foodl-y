                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.02"
                        android:singleLine="true"
                        android:text="@{singleRestaurantVoneVM.singleRestaurantVoneModel.txtFeaturedItems}"
                        style="@style/txtSFProDisplaymedium20" tools:text="@string/lbl_featured_items"/>
                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/recyclerListbg"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="match_parent"
                        android:nestedScrollingEnabled="false"
                        android:orientation="horizontal"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        itemSpace="@{@dimen/_14pxh}" tools:itemCount="3" tools:listitem="@layout/row_listbg"/>
                </LinearLayout>
                <HorizontalScrollView android:fillViewport="true"
                    android:gravity="start"
                    android:id="@+id/horizontalScrollHorizontalscrollbeeflamb"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_38pxv"
                    android:layout_width="match_parent" android:scrollbars="none">
                    <LinearLayout android:id="@+id/linearMenu"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtBeefLamb"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.25"
                            android:layout_width="0dp"
                            android:letterSpacing="0.01"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{singleRestaurantVoneVM.singleRestaurantVoneModel.txtBeefLamb}"
                            style="@style/txtSFProDisplaysemibold24_2" tools:text="@string/lbl_beef_lamb"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSeafood"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.3"
                            android:layout_width="0dp"
                            android:letterSpacing="0.01"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{singleRestaurantVoneVM.singleRestaurantVoneModel.txtSeafood}"
                            style="@style/txtSFProDisplaysemibold24" tools:text="@string/lbl_seafood"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAppetizers"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_weight="0.39"
                            android:layout_width="0dp"
                            android:letterSpacing="0.01"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{singleRestaurantVoneVM.singleRestaurantVoneModel.txtAppetizers}"
                            style="@style/txtSFProDisplaysemibold24_2" tools:text="@string/lbl_appetizers"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDimSum"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_weight="0.32"
                            android:layout_width="0dp"
                            android:letterSpacing="0.01"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{singleRestaurantVoneVM.singleRestaurantVoneModel.txtDimSum}"
                            style="@style/txtSFProDisplaysemibold24_2" tools:text="@string/lbl_dim_sum2"/>
                    </LinearLayout>
                </HorizontalScrollView>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMostPopulars"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.02"
                    android:singleLine="true"
                    android:text="@{singleRestaurantVoneVM.singleRestaurantVoneModel.txtMostPopulars}"
                    style="@style/txtSFProDisplaymedium20" tools:text="@string/lbl_most_populars"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:gravity="center_horizontal"
                    android:id="@+id/recyclerListfood"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_width="match_parent"
                    android:nestedScrollingEnabled="false"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_41pxh}" tools:itemCount="3" tools:listitem="@layout/row_listfood"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSeaFoods"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.02"
                    android:singleLine="true"
                    android:text="@{singleRestaurantVoneVM.singleRestaurantVoneModel.txtSeaFoods}"
                    style="@style/txtSFProDisplaymedium20" tools:text="@string/lbl_sea_foods"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:gravity="center_horizontal"
                    android:id="@+id/recyclerListfoodThree"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="match_parent"
                    android:nestedScrollingEnabled="false"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_41pxh}" tools:itemCount="3" tools:listitem="@layout/row_listfood_three"/>
