                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginStart="@dimen/_1pxh"
                                android:layout_width="match_parent" android:orientation="horizontal">
                                <ImageView android:id="@+id/imageShare"
                                    android:layout_height="@dimen/_20pxh"
                                    android:layout_width="@dimen/_20pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_share"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_share"/>
                                <ImageView android:id="@+id/imageSearch"
                                    android:layout_height="@dimen/_17pxh"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:layout_marginStart="@dimen/_26pxh"
                                    android:layout_marginTop="@dimen/_2pxv"
                                    android:layout_width="@dimen/_16pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_search_white_a700"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_search_white_a700"/>
                            </LinearLayout>

