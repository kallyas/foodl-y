<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="confirmPhoneNumberVM" type="com.foodly.app.modules.confirmphonenumber.data.viewmodel.ConfirmPhoneNumberVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linear11Confirmphon"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearBarsTopbars1"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="@dimen/_8pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLogintoFoodly"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginEnd="@dimen/_130pxh"
                    android:layout_marginStart="@dimen/_95pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_weight="0.93"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{confirmPhoneNumberVM.confirmPhoneNumberModel.txtLogintoFoodly}"
                    style="@style/txtSFProTextsemibold16" tools:text="@string/lbl_login_to_foodly"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <TextView android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtVerifyphonenu"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_width="match_parent"
            android:letterSpacing="0.01"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@{confirmPhoneNumberVM.confirmPhoneNumberModel.txtVerifyphonenu}"
            style="@style/txtSFProDisplaysemibold24" tools:text="@string/msg_verify_phone_nu"/>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtEnterthe4Dig"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_8pxh"
            android:text="@{confirmPhoneNumberVM.confirmPhoneNumberModel.txtEnterthe4Dig}"
            style="@style/txtSFProTextregular16_3" tools:text="@string/msg_enter_the_4_dig"/>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearEntercode"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearCodeFill"
                android:layout_height="wrap_content"
                android:layout_weight="0.25" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStylewhite_5">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFour"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{confirmPhoneNumberVM.confirmPhoneNumberModel.txtFour}"
                    style="@style/txtSFProTextmedium16" tools:text="@string/lbl_4"/>
                <View android:background="@color/bluegray_400"
                    android:id="@+id/lineRectangleCopy"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_15pxv" android:layout_width="@dimen/_50pxh"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageComputer"
                android:layout_height="@dimen/_50pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_width="@dimen/_50pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_computer"
                tools:ignore="ContentDescription" tools:src="@drawable/img_computer"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearCodeDeactive"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_weight="0.25" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStylewhite_5">
                <View android:background="@color/black_900_88"
                    android:id="@+id/lineRectangleCopyOne"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_49pxv" android:layout_width="@dimen/_50pxh"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearCodeDeactive1"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_weight="0.25" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStylewhite_5">
                <View android:background="@color/black_900_88"
                    android:id="@+id/lineRectangleCopyTwo"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_49pxv" android:layout_width="@dimen/_50pxh"/>
            </LinearLayout>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnContinue"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_34pxv"
            android:layout_width="match_parent"
            android:letterSpacing="0.04"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:paddingBottom="@dimen/_17pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_17pxv" android:singleLine="true"
            android:text="@string/lbl_continue"
            android:textAllCaps="true" style="@style/btnSolidRounded_2" tools:text="@string/lbl_continue"/>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearResendagain"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtDidntreceive"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_weight="0.59" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{confirmPhoneNumberVM.confirmPhoneNumberModel.txtDidntreceive}"
                style="@style/txtSFProTextmedium12_1" tools:text="@string/msg_didn_t_receive"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtResendAgain"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.41" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{confirmPhoneNumberVM.confirmPhoneNumberModel.txtResendAgain}"
                style="@style/txtSFProTextmedium12_2" tools:text="@string/lbl_resend_again"/>
        </LinearLayout>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtTermAndCondition"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_31pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_8pxh"
            android:text="@{confirmPhoneNumberVM.confirmPhoneNumberModel.txtTermAndCondition}"
            style="@style/txtSFProTextregular16" tools:text="@string/msg_by_signing_up_y"/>
    </LinearLayout>
</layout>
